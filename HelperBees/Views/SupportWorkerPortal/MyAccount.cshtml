
@{
    ViewData["Title"] = "My Account";
    Layout = "~/Views/Shared/_SupportCoordinator.cshtml";
}

<h1>My Account</h1>

<div style="text-align:center;">
    <h3>Account Settings</h3>
    <input type="text" placeholder="First Name" /><br />
    <input type="text" placeholder="Last Name" /><br />
    <input type="text" placeholder="Organisation" /><br />
    <input type="text" placeholder="Email Address" /><br />
    <input type="text" placeholder="Mobile Number" /><br />
    <input type="text" placeholder="Password" /><br />
    <input type="text" placeholder="Re-enter Password" /><br />
    <br />
    <input type="button" value="Update My Account" onclick="window.location = '/supportcoordinator/dashboard'" />
</div>
